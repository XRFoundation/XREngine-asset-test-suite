{
  "name": "ee-development-test-suite",
  "version": "0.0.0",
  "description": "Assets and tests for Ethereal Engine core development",
  "main": "",
  "scripts": {
    "quick-test": "mocha --config .mocharc.js --spec 'e2e/dev/*.test.ts'",
    "check-errors": "tsc --noemit",
    "test": "mocha --config .mocharc.js --spec 'tests/**/*.test.ts'",
    "test-e2e": "mocha --config .mocharc.js --spec 'e2e/**/*.test.ts'",
    "benchmark": "mocha --config .mocharc.js --timeout 3700000 --spec 'e2e/benchmark/basic.test.ts'",
    "precommit": "no-master-commits -b main",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "format-scss": "stylelint \"**/*.scss\" --fix",
    "format-staged": "lint-staged",
    "quick-test-browserstack": "browserstack-node-sdk mocha --config .mocharc.js --spec 'e2e/dev/*.test.ts'",
    "test-browserstack": "browserstack-node-sdk mocha --config .mocharc.js --spec 'tests/**/*.test.ts'",
    "benchmark-browserstack": "browserstack-node-sdk mocha e2e/benchmark/basic.test.js"
  },
  "etherealEngine": {
    "version": "1.6.0"
  },
  "pre-commit": [
    "format-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "browserstack-local": "^1.4.8"
  },
  "devDependencies": {
    "browserstack-node-sdk": "latest"
  }
}